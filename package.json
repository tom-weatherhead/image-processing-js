{
	"name": "image-processing-js",
	"version": "0.0.5",
	"description": "A pure JavaScript raster image processing engine.",
	"main": "src/main.js",
	"homepage": "https://github.com/tom-weatherhead/image-processing-js#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tom-weatherhead/image-processing-js.git"
	},
	"bugs": {
		"url": "https://github.com/tom-weatherhead/image-processing-js/issues"
	},
	"scripts": {
		"all": "npm run clean && npm run build",
		"build": "npm i && grunt",
		"clean": "rm -rf node_modules && rm -f package-lock.json",
		"lint": "grunt eslint",
		"mocha": "grunt mochaTest",
		"start": "node src/cli.js",
		"test": "grunt"
	},
	"bin": {
		"ipjs": "src/cli.js"
	},
	"keywords": [
		"compositing",
		"convolution",
		"flip",
		"Gaussian blur",
		"jpeg",
		"mirror",
		"pixelate",
		"resample",
		"rotate"
	],
	"dependencies": {
		"thaw-image-utilities.js": "^0.0.4"
	},
	"devDependencies": {
		"grunt": "^1.0.4",
		"grunt-eslint": "^22.0.0"
	},
	"author": "Tom Weatherhead <null@2hrd4u.org> (https://httpbin.org/status/418)",
	"license": "MIT"
}
