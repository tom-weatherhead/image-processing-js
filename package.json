{
	"name": "image-processing-js",
	"version": "0.0.6",
	"description": "A pure JavaScript raster image processing engine.",
	"homepage": "https://github.com/tom-weatherhead/image-processing-js#readme",
	"repository": "github:tom-weatherhead/image-processing-js",
	"bugs": {
		"url": "https://github.com/tom-weatherhead/image-processing-js/issues"
	},
	"main": "src/main.js",
	"files": [
		"src/**/*"
	],
	"scripts": {
		"all": "npm run clean && npm run build",
		"build": "npm i && grunt",
		"clean": "rm -rf node_modules && rm -f package-lock.json",
		"lint": "grunt eslint",
		"start": "node src/cli.js",
		"test": "grunt"
	},
	"bin": {
		"ipjs": "src/cli.js"
	},
	"keywords": [
		"bicubic",
		"bilinear",
		"compositing",
		"convolution",
		"flip",
		"Gaussian blur",
		"image processing",
		"jpeg",
		"mirror",
		"pixelate",
		"raster",
		"resample",
		"resampling",
		"rotate"
	],
	"dependencies": {
		"thaw-image-utilities.js": "^0.0.6"
	},
	"devDependencies": {
		"babel-mixin": "^0.1.0",
		"jest": "^26.4.0"
	},
	"author": "Tom Weatherhead <null@2hrd4u.org> (https://github.com/tom-weatherhead)",
	"license": "MIT"
}
